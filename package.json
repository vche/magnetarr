{
  "name": "magnetarr",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/chrome": "^0.0.263",
    "nodemon": "^3.1.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.13",
    "parcel": "^2.12.0",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "targets": {
    "background": {
      "source": "src/background/background.js",
      "distDir": "release/src/background",
      "publicUrl": "./"
    },
    "opt": {
      "source": "src/opt/options.html",
      "distDir": "release/src/opt",
      "publicUrl": "./"
    },
    "popup": {
      "source": "src/popup/popup.html",
      "distDir": "release/src/popup",
      "publicUrl": "./"
    }
  },
  "scripts": {
    "start": "parcel",
    "build": "npm run clean; parcel build",
    "dev": "nodemon --exec npm run build",
    "clean": "rm -r release/src/*"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "release/",
      ".cache/"
    ],
    "delay": 500,
    "watch": [
      "src/"
    ],
    "ext": "js,ts,tsx,css,json,html,png"
  }
}
